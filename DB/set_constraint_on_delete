INSERT INTO T_BOARD VALUES (2, '제목', '내용입니당', 3);
COMMIT;

CREATE TABLE T_BOARD_LIKE (
    I_BOARD NUMBER,
    I_STUDENT NUMBER,  
    CONSTRAINT FK_I_BOARD
    FOREIGN KEY (I_BOARD) REFERENCES T_BOARD(I_BOARD) ON DELETE CASCADE
);
DROP TABLE T_BOARD_LIKE;
INSERT INTO T_BOARD_LIKE VALUES (1, 3);
INSERT INTO T_BOARD VALUES (3, '테스트', '이것은 테스트입니다', 1);
COMMIT;

ALTER TABLE T_BOARD_LIKE DROP CONSTRAINT FK_I_BOARD;

ALTER TABLE T_BOARD_LIKE 
DROP CONSTRAINT FK_I_BOARD;

ALTER TABLE T_BOARD_LIKE
ADD CONSTRAINT FK_I_BOARD FOREIGN KEY (I_BOARD) REFERENCES T_BOARD(I_BOARD) ON DELETE CASCADE;
COMMIT;

-- fk name 모르면 r_constraint_name 로 주면 됌 sys_c0011248 이런식 

